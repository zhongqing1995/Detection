.bottom-tab {
  position: fixed;
  bottom: 0;
  width: 100%;
  display: flex;
  justify-content: space-between;
  height: 55px;
  box-sizing: border-box;
  padding-top: 10px;
  background: rgba(255, 255, 255, 1);
  box-shadow: 0px -5px 26px 0px rgba(0, 0, 0, 0.05);

}

.bottom-tab>view {
  width: 20%;
  text-align: center;
  font-family: PingFang;
  font-weight: 400;
  position: relative;
  line-height: 17px;

}

.bottom-tab>view>view:nth-child(1) {
  width: 22px;
  height: 22px;
  margin: 0 auto;

}



.bottom-tab>view:nth-child(1)>view:nth-child(1)  {
  animation: bounce 1.5s;
}

.tab-button>view:nth-child(1){
  animation: bounce 1.7s;
}

/* .bottom-tab>view:nth-child(3)>view:nth-child(1) {
  animation: bounceInDown 3s;
} */

.bottom-tab>view:nth-child(4)>view:nth-child(1) {
  animation: bounce 2.1s;
}

.bottom-tab>view:nth-child(5)>view:nth-child(1) {
  animation: bounce 2.3s;
}
.bottom-tab>view>view:nth-child(1) span {
  font-size: 22px;
}

.bottom-tab>view>view:nth-child(2) {
  margin-top: 2px;
  font-size: .76rem;
}

.bottom-tab image {
  width: 100%;
  height: 100%;

}

.xzsb {
  color: rgba(102, 102, 102, 1) !important;

}

.xzcg {
  color: rgba(37, 117, 244, 1);

}
.wz1{
  color: rgba(102, 102, 102, 1) !important;
}
.tab-zxy {
  width: 55px !important;
  height: 55px !important;
  box-sizing: border-box;
  padding-top: 12px;
  text-align: center;
  color: #fff !important;
  background: rgba(37, 117, 244, 1);
  border: 4px solid #c7dbfc;
  border-radius: 50%;
  font-size: 26px !important;
  margin-top: -35px !important;
  margin-bottom: 10px;
  animation: blockquote 4s infinite linear ;
}

.m {
  animation: fadeIn .8s;
  box-sizing: border-box;
}

.tab-zxy span {
  font-size: 26px;
}

.tab-zxy-wz {
  margin-top: 4px !important;
}

.bottom-tab button {
  width: 20%!important;
  border: 0px;
  height: auto;
  background: transparent;
  margin: 0px;
  padding: 0px;
  overflow: visible;
  font-weight: initial;
  display: initial;
  line-height: initial;
  margin-top: -3px;
  position: initial;
  overflow: hidden;

}



.bottom-tab .icon-zaixianzixun {
  font-size: 24px;
  color: rgba(102, 102, 102, 1) !important;


}

.tab-button>view:nth-child(1) {
  width: 22px;
  height: 22px;
  margin: 0 auto;

}

.tab-button>view:nth-child(2) {
  margin-top: 5px;
  font-size: .76rem;
  color: rgba(102, 102, 102, 1) !important;

}

button:not {
  width: auto !important;
}

@keyframes bc2 {
  0% {
    opacity: 0;
  }
  50% {
    opacity: 0;
  }
  100% {
    opacity: 1;
  }
}

@keyframes bc {
  0% {
    background: rgba(37, 117, 244, 1);
    color: rgba(37, 117, 244, 1);
  }
  50% {
    background: rgba(37, 117, 244, 1);
    color: rgba(37, 117, 244, 1);
  }
  100% {}
}

@keyframes blockquote {

  0% {
    transform: scale(1);
  }

  25% {
    transform: scale(0.9);
  }
  50% {
    transform: scale(.8);
  }
  75% {
    transform: scale(.9);
  }

  100% {
    transform: scale(1);
  }
