.home {
  padding-bottom: 60px;
}

.home-top {
  height: 135px;
  padding-top: 14px;
  background: rgba(37, 117, 244, 1);
  display: flex;
  /* border-radius: 0px 0px 14px 14px; */
  justify-content: space-between;
}

.home-top image {
  width: 100%;
  height: 100%;

}

.home-top>view {
  width: 25%;
  text-align: center;
}

.home-top>view>view:nth-child(1) {
  width: 51px;
  height: 39px;
  margin: 0 auto;
  font-size: 32px;
  color: #fff;
  margin-bottom: 6px;
}
.home-top>view:nth-child(1)>view:nth-child(1) {
  animation: 1s fadeInLeft  ;
}
.home-top>view:nth-child(2)>view:nth-child(1) {
  animation: 1.3s  fadeInLeft  ;
}
.home-top>view:nth-child(3)>view:nth-child(1) {
  animation: 1.6s fadeInLeft  ;
}
.home-top>view:nth-child(4)>view:nth-child(1) {
  animation: 1.9s  fadeInLeft  ;
}

.home-jfg2>view:nth-child(1)>view:nth-child(1) {
  animation: 1s fadeInRight  ;
}
.home-jfg2>view:nth-child(2)>view:nth-child(1) {
  animation: 1.3s  fadeInRight   ;
}
.home-jfg2>view:nth-child(3)>button>view:nth-child(1) {
  animation: 1.6s fadeInRight   ;
}
.home-jfg2>view:nth-child(4)>view:nth-child(1) {
  animation: 1.9s  fadeInRight   ;
}

.home-top>view>view:nth-child(1) span {
  font-size: 30px;
}


.home-top>view>view:nth-child(2) {
  font-size: .9rem;
  font-family: PingFang;
  color: rgba(255, 255, 255, 1);
}

.home-jfg {
  margin: 10px;
  margin-top: -43px;
  overflow: hidden;
  position: relative;
  z-index: 100;
  border-radius: 8px 8px 8px 8px;
}

.home-jfg>view:nth-child(1) {
  height: 111px;
  overflow: hidden;
}

.home-jfg>view:nth-child(3) {
  display: flex;
  justify-content: space-between;
  padding: 15px;
  padding-right: 0px;
  padding-left: 0px;
  background: #fff;
  padding-top: 25px;

}

.home-jfg>view:nth-child(2) {
  width: 93px;
  height: 32px;
  background: rgba(244, 158, 0, 1);
  border-radius: 16px;
  color: #fff;
  text-align: center;
  margin: 0 auto;
  padding-top: 7px;
  margin-top: -15px;
  position: absolute;
  left: calc(50% - 46.5px);
}

.home-jfg>view:nth-child(3) view>view:nth-child(1) {
  text-align: center;
  color: rgba(244, 158, 0, 1);
  margin-bottom: 6px;
height: 29px;
}
.po{
