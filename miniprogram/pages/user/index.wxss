/* miniprogram/pages/user/index.wxss */
.user-top {
  height: 110px;
  background: rgba(37, 117, 244, 1);
  position: relative;

  overflow: hidden;
}
.bg_ware {
  position: absolute;
  left: 0;
  bottom: -2rpx;
  width: 100%;
  mix-blend-mode: screen;
  height: 86rpx;

}

.user-top {
  display: flex;
  padding: 15px;
}

.user-top>view:nth-child(1) {
  width: 57px;
  height: 57px;
  border-radius: 50%;
  overflow: hidden;
}

.user-wz>view:nth-child(1) {
  font-size: 1.1rem;
  font-family: PingFang;
  font-weight: bold;
  color: rgba(255, 255, 255, 1);
}

.user-wz {
  padding-left: 20px;
}

.user-wz>view:nth-child(2) {
  display: flex;
  margin-top: 7px;
}

.user-wz>view:nth-child(2) view {
  margin-right: 10px;
  height: 24px;
  border: 1px solid #c7dbfc;
  border-radius: 16px;
  font-size: 0.6rem;
  padding: 2px 10px;
  font-size: 0.7rem;
  color: #fff;
}
 .user-xzlb button{
  width:100%;
  height: auto;
  background: transparent;
  margin: 0px;
  padding: 0px;
  overflow: visible;
  font-weight: initial;
  display: initial;
  display: flex;
  line-height: initial;


  
}
.user van-cell{
  width: 100%;
}
.user-jgg {
  height: 80px;
  background: rgba(255, 255, 255, 1);
  border-radius: 6px;
  padding-top: 12px;
  display: flex;
  justify-content: space-between;

  margin: 15px;
}

.user-jgg>view>view {
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  margin-top: 5px;
  font-size: 0.8rem;

}

.user-jgg span {
  font-size: 22px;
}

.user-jgg>view {
  padding: 5px;
  width: 20%;
  color: #CCCCCCFF;
  text-align: center;
  margin-bottom: 10px;
}
.user{
  animation: fadeIn 1.5s;
}
.user-xzlb>view>span {
  float: left;

}
.user-lf van-icon{
  color: #22CE9FFF;
font-size: 20px;
width: 16px;

margin-right: 10px;

}
.user-xzlb>view>view {
  float: right;

}

.user-lf {
