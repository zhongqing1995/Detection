.main-seach-bh {
  font-size: 12px;
  font-family: PingFang;
  font-weight: bold;
  color: rgba(51, 51, 51, 1);
  display: flex;
  justify-content: space-between;
}

.main-seach {
  margin: 10px;
  background-color: #fff;
  padding: 10px;
  animation:  fadeInRight 1s;

}

.main-seach-bh .icon-bianhao {
  font-size: 30px;
  color: #F49E00FF;
}

.main-seach-bh>view:nth-child(1) {
  color: #F49E00FF;
  font-size: 24px;
  display: flex;
  margin-bottom: 6px;
}

.main-seach-bh>view:nth-child(1) view:nth-child(2) {
  margin-top: 1px;
  margin-left: 5px;
  color: rgba(51, 51, 51, 1);
  font-size: 32rpx;
}

.main-seach-wz1 span {
  color: #FE0000FF;
}

.main-seach-wz1 {
  display: flex;

  padding: 3px;
}

.main-seach-wz1>view:nth-child(1) {

  font-family: PingFang;
  font-weight: 400;
  color: rgba(153, 153, 153, 1);
  line-height: 24px;
}

.main-seach-wz1>view:nth-child(2) {
  margin-left: 10px;
  font-family: PingFang;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  line-height: 24px;
}

.main-seach-wz2 {
  display: flex;
  margin-top: 8px;
  justify-content: flex-end;
}

.main .van-tab--active {
  color: rgba(244, 158, 0, 1);
}


.main .van-tabs__line {
  color: rgba(244, 158, 0, 1);
  background-color: rgba(244, 158, 0, 1);
}
.main .van-tab--active {
  color: rgba(17, 111, 198, 1);
}

.main .van-tabs__line {
  color: rgba(17, 111, 198, 1);
  background-color: rgba(17, 111, 198, 1);
}

.main .van-search__content {
  border-radius: 12px;
  overflow: hidden;
}

.main-seach-wz2>view {
  padding: 5px 15px;
  background: rgba(255, 255, 255, 1);
  border: 1px solid rgba(230, 230, 230, 1);
  border-radius: 17px;
  text-align: center;
  color: rgba(153, 153, 153, 1);
  font-size: 13px;
  padding-top: 4px;
  margin: 0px 5px;
}

.main-seach-wz2>view:last-child {
  border: 1px solid rgba(244, 158, 0, 1);
  color: rgba(244, 158, 0, 1);
}

.main .van-tab--active {
  color: rgba(244, 158, 0, 1);
}


.main .van-tabs__line {
  color: rgba(244, 158, 0, 1);
  background-color: rgba(244, 158, 0, 1);
}

.buttom {
  text-align: center;
  padding-top: 2px;
  border-radius: 12px;
}

.main-seach-bh .van-icon {
  background: rgba(244, 158, 0, 1);
  padding: 3px;
  font-size: 16px;
  border-radius: 5px;
  color: #fff;
}

.ys1 {
  color: #F46700FF;
}

.ys2 {
  color: #F49E00FF
}

.ys3 {
  color: #2575F4FF
}

